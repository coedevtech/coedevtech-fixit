{
    "name": "coedevtech/fixit",
    "description": "A Laravel package that captures and logs all exceptions into a database table â€” with optional encryption, email alerts, and a powerful CLI interface. Designed to give you full visibility into unhandled errors, without clutter or guesswork",
    "keywords": [
        "laravel",
        "error logger",
        "exception logger",
        "fixit",
        "debugging",
        "logging"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "onyiimesi",
            "email": "onyedikachukwu62@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "orchestra/testbench": "*",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Fixit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fixit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fixit\\FixitServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
